<?xml version="1.0"?>

<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

	<!-- list all the packages being supplied by this jar -->
	<RDF:Seq about="urn:mozilla:package:root">
		<RDF:li resource="urn:mozilla:package:rubysupport"/>
	</RDF:Seq>

	<!-- package information -->
	<RDF:Description about="urn:mozilla:package:rubysupport"
		chrome:displayName="XHTML Ruby Support"
		chrome:author="SHIMODA Hiroshi"
		chrome:name="rubysupport"
		chrome:settingsURL="chrome://rubysupport/content/prefDialog.xul"
		chrome:extension="true"
		chrome:description="Adds XHTML Ruby support. Ruby texts will be rendered above the base texts, as a small text."/>


	<!-- overlay information -->
	<RDF:Seq about="urn:mozilla:overlays">
		<RDF:li resource="chrome://global/content/globalOverlay.xul"/>
		<RDF:li resource="chrome://communicator/content/pref/preftree.xul"/>

		<!-- Firefox -->
		<RDF:li resource="chrome://browser/content/browser.xul"/>
	</RDF:Seq>


	<RDF:Seq about="chrome://global/content/globalOverlay.xul">
		<RDF:li>chrome://rubysupport/content/globalOverlay.xul</RDF:li>
	</RDF:Seq>
	<RDF:Seq about="chrome://communicator/content/pref/preftree.xul">
		<RDF:li>chrome://rubysupport/content/preftreeOverlay.xul</RDF:li>
	</RDF:Seq>

	<!-- Firefox -->
	<RDF:Seq about="chrome://browser/content/browser.xul">
		<RDF:li>chrome://rubysupport/content/globalOverlay.xul</RDF:li>
	</RDF:Seq>

</RDF:RDF>
